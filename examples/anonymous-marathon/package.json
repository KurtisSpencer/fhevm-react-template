{
  "name": "anonymous-marathon-example",
  "version": "1.0.0",
  "description": "Privacy-preserving marathon registration platform using Universal FHEVM SDK",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "test:gas": "REPORT_GAS=true hardhat test",
    "test:coverage": "hardhat coverage",
    "deploy:sepolia": "hardhat run scripts/deploy.js --network sepolia",
    "verify:sepolia": "hardhat run scripts/verify.js --network sepolia",
    "interact": "hardhat run scripts/interact.js --network sepolia",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build"
  },
  "keywords": [
    "fhevm",
    "marathon",
    "privacy",
    "encryption",
    "blockchain",
    "zama"
  ],
  "author": "FHEVM SDK Contributors",
  "license": "MIT",
  "dependencies": {
    "@fhevm/solidity": "^0.5.0",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "hardhat": "^2.19.0",
    "ethers": "^6.10.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-gas-reporter": "^1.0.9",
    "solhint": "^4.1.0",
    "prettier": "^3.1.1",
    "prettier-plugin-solidity": "^1.3.0"
  }
}
